Proceso multiplicacion
	Definir numero1, numero2 Como Entero;
	Definir resultado Como Entero;
	Escribir '¿Cuál es tu primer número?';
	Leer numero1;
	Escribir '¿Cuál es tu segundo número?';
	Leer numero2;
	resultado <- numero1*numero2;
	Escribir ' El resultado de tu multiplicación es ', resultado;
FinProceso
